{
  "/ENTRY[entry]":{
    "depends_on": "/entry/sample_stage_cs_transformations/vamas_sample_stage_coordinate_system",
    "TRANSFORMATIONS[sample_stage_cs_transformations]": {
      "AXISNAME[vamas_sample_stage_coordinate_system]":null,
      "AXISNAME[vamas_sample_stage_coordinate_system]/@depends_on":"y_rotation",
      "AXISNAME[vamas_sample_stage_coordinate_system]/@direction":"X-axis points in sample stage x direction, Y-Axis points in sample stage y direction (towards the operator), Z-axis points upwards, forming a left-handed coordinate_system",
      "AXISNAME[vamas_sample_stage_coordinate_system]/@matrix": [[1, 0, 0], [0, 1, 0], [0, 0, 1]], 

      "AXISNAME[y_rotation]":"@xps_token:beam_xray/source_polar_angle",
      "AXISNAME[y_rotation]/@depends_on":"y_flip",
      "AXISNAME[y_rotation]/@transformation_type": "rotation",
      "AXISNAME[y_rotation]/@units":"@xps_token:beam_xray/source_polar_angle/@units",
      "AXISNAME[y_rotation]/@vector": [0, -1, 0],      
      
      "AXISNAME[y_flip]":180,
      "AXISNAME[y_flip]/@depends_on":"z_rotation",
      "AXISNAME[y_flip]/@transformation_type": "rotation",
      "AXISNAME[y_flip]/@units":"degree",
      "AXISNAME[y_flip]/@vector": [0, 1, 0],

      "AXISNAME[z_rotation]":"@xps_token:beam_xray/source_azimuth_angle",
      "AXISNAME[z_rotation]/@depends_on":".",
      "AXISNAME[z_rotation]/@transformation_type": "rotation",
      "AXISNAME[z_rotation]/@units":"@xps_token:beam_xray/source_azimuth_angle/@units",
      "AXISNAME[z_rotation]/@vector": [0, 0, 1],

      "AXISNAME[base_change]":1,
      "AXISNAME[base_change]/@depends_on":".",
      "AXISNAME[base_change]/@transformation_type": "base_change",
      "AXISNAME[base_change]/@matrix": [[-1, 0, 0], [0, 1, 0], [0, 0, 1]]  
    }
  },
  "/ENTRY[entry]/INSTRUMENT[instrument]":{
    "beamTYPE[beam_xray]":{
      "depends_on": "/entry/instrument/beam_xray/transformations/incoming_beam_vector",
      
      "TRANSFORMATIONS[transformations]": {
        "AXISNAME[incoming_beam_vector]":1,
        "AXISNAME[incoming_beam_vector]/@depends_on":"beam_polar_angle",      
        "AXISNAME[incoming_beam_vector]/@transformation_type": "translation",
        "AXISNAME[incoming_beam_vector]/@units":"m",
        "AXISNAME[incoming_beam_vector]/@vector": [0, 0, 1],
      
        "AXISNAME[beam_polar_angle]":"@xps_token:beam_xray/source_polar_angle",
        "AXISNAME[beam_polar_angle]/@depends_on":"beam_azimuth_angle",
        "AXISNAME[beam_polar_angle]/@transformation_type": "rotation",
        "AXISNAME[beam_polar_angle]/@units":"@xps_token:beam_xray/source_polar_angle/@units",
        "AXISNAME[beam_polar_angle]/@vector": [1, 0, 0],
     
        "AXISNAME[beam_azimuth_angle]":"@xps_token:beam_xray/source_azimuth_angle",
        "AXISNAME[beam_azimuth_angle]/@depends_on":"entry/geometries/xps_coordinate_system/coordinate_transformations/beam_axis_to_analyser_axis",
        "AXISNAME[beam_azimuth_angle]/@transformation_type": "rotation",
        "AXISNAME[beam_azimuth_angle]/@units":"@xps_token:beam_xray/source_azimuth_angle/@units",
        "AXISNAME[beam_azimuth_angles]/@vector": [0, 0, -1]
      }
    },             
    "ELECTRONANALYSER[electronanalyser]":{
      "depends_on": "entry/instrument/electronanalyser/transformations/analyzer_central_axis",
      "TRANSFORMATIONS[transformations]": {
        "AXISNAME[analyzer_central_axis]":1,
        "AXISNAME[analyzer_central_axis]/@depends_on":"analyzer_polar_angle",      
        "AXISNAME[analyzer_central_axis]/@transformation_type": "translation",
        "AXISNAME[analyzer_central_axis]/@units":"m",
        "AXISNAME[analyzer_central_axis]/@vector": [0, 0, 1],

        "AXISNAME[analyser_take_off_polar_angle]":"@xps_token:analyser/analyser_take_off_polar_angle",
        "AXISNAME[analyser_take_off_polar_angle]/@depends_on":"analyzer_azimuth_angle",
        "AXISNAME[analyser_take_off_polar_angle]/@transformation_type": "rotation",
        "AXISNAME[analyser_take_off_polar_angle]/@units":"@xps_token:analyser/analyser_take_off_polar_angle/@units",
        "AXISNAME[analyser_take_off_polar_angle]/@vector": [1, 0, 0],

        "AXISNAME[analyzer_azimuth_angle]":"@xps_token:analyser/analyzer_azimuth_angle",
        "AXISNAME[analyzer_azimuth_angle]/@depends_on":".",
        "AXISNAME[analyzer_azimuth_angle]/@transformation_type": "rotation",
        "AXISNAME[analyzer_azimuth_angle]/@units":"@xps_token:beam_xray/analyzer_azimuth_angle/@units",
        "AXISNAME[analyzer_azimuth_angle]/@vector": [0, 0, -1]
      }
    }
  }, 
  "/ENTRY[entry]/SAMPLE[sample]":{
    "depends_on":"/entry/sample/transformations/sample_normal",
    "TRANSFORMATIONS[transformations]": {
      "AXISNAME[sample_normal]":1,
      "AXISNAME[sample_normal]/@depends_on":"sample_rotation",      
      "AXISNAME[sample_normal]/@transformation_type": "translation",
      "AXISNAME[sample_normal]/@units":"m",
      "AXISNAME[sample_normal]/@vector": [0, 0, 1], 

      "AXISNAME[sample_rotation_angle]":"@xps_token:sample/sample_rotation",
      "AXISNAME[sample_rotation_angle]/@depends_on":"sample_normal_polar_angle_of_tilt",
      "AXISNAME[sample_rotation_angle]/@transformation_type": "rotation",
      "AXISNAME[sample_rotation_angle]/@units":"@xps_token:sample/sample_rotation/@units",
      "AXISNAME[sample_rotation_angle]/@vector": [0, 0, -1],

      "AXISNAME[sample_normal_polar_angle_of_tilt]":"@xps_token:sample/sample_normal_polar_angle_of_tilt",
      "AXISNAME[sample_normal_polar_angle_of_tilt]/@depends_on":"sample_normal_tilt_azimuth",
      "AXISNAME[sample_normal_polar_angle_of_tilt]/@transformation_type": "rotation",
      "AXISNAME[sample_normal_polar_angle_of_tilt]/@units":"@xps_token:sample/sample_normal_polar_angle_of_tilt/@units",
      "AXISNAME[sample_normal_polar_angle_of_tilt]/@vector": [1, 0, 0],

      "AXISNAME[sample_normal_tilt_azimuth_angle]":"@xps_token:sample/sample_normal_tilt_azimuth",
      "AXISNAME[sample_normal_tilt_azimuth_angle]/@depends_on":".",
      "AXISNAME[sample_normal_tilt_azimuth_angle]/@transformation_type": "rotation",
      "AXISNAME[sample_normal_tilt_azimuth_angle]/@units":"@xps_token:sample/sample_normal_tilt_azimuth/@units",
      "AXISNAME[sample_normal_tilt_azimuth_angle]/@vector": [0, 0, -1]
    }
  }
}