{
   "/ENTRY[entry]/geometries":{
      "COORDINATE_SYSTEM[xps_coordinate_system]":{
        "origin":"sample stage",
        "handedness":"left_handed",
        "z_direction":"sample stage normal",
        "depends_on": "/entry/geometries/xps_coordinate_system/coordinate_transformations/beam_axis_to_analyser_axis",
        "TRANSFORMATIONS[coordinate_transformations]": {
          "AXISNAME[beam_axis_to_analyser_axis]":"@xps_token:beam_xray/source_azimuth_to_sample",
          "AXISNAME[beam_axis_to_analyser_axis]/@depends_on":".",
          "AXISNAME[beam_axis_to_analyser_axis]/@transformation_type": "rotation",
          "AXISNAME[beam_axis_to_analyser_axis]/@units":"@xps_token:beam_xray/source_azimuth_to_sample/@units",
          "AXISNAME[beam_axis_to_analyser_axis]/@vector": [0, 0 , -1]
       }
     }
   },
   "/ENTRY[entry]/INSTRUMENT[instrument]":{
     "sourceTYPE[source_xray]":{
       },
       "depends_on": "/entry/instrument/source_xray/transformations/source_azimuth_to_sample",
       "TRANSFORMATIONS[transformations]": {
         "AXISNAME[source_azimuth_to_sample]":"@xps_token:source_xray/source_azimuth",
         "AXISNAME[source_azimuth_to_sample]/@depends_on":"entry/geometries/xps_coordinate_system/coordinate_transformations/beam_axis_to_analyser_axis",
         "AXISNAME[source_azimuth_to_sample]/@transformation_type": "rotation",
         "AXISNAME[source_azimuth_to_sample]/@units":"@xps_token:source_xray/source_azimuth/@units",
         "AXISNAME[source_azimuth_to_sample]/@vector": [0, 1, 0]
       },
     "ELECTRONANALYSER[electronanalyser]":{
       "depends_on": "transformations/analyser_take_off_azimuth",
       "TRANSFORMATIONS[transformations]": {
         "AXISNAME[analyser_take_off_azimuth]":"@xps_token:instrument/analyser/analyser_take_off_azimuth",
         "AXISNAME[analyser_take_off_azimuth]/@depends_on":"analyser_take_off_polar",      
         "AXISNAME[analyser_take_off_azimuth]/@transformation_type": "rotation",
         "AXISNAME[analyser_take_off_azimuth]/@units":"@xps_token:instrument/analyser/analyser_take_off_azimuth/@units",
         "AXISNAME[analyser_take_off_azimuth]/@vector": [0, 1, 0],

         "AXISNAME[analyser_take_off_polar]":"@xps_token:instrument/analyser/analyser_take_off_polar",
         "AXISNAME[analyser_take_off_polar]/@depends_on":"/entry/geometries/xps_coordinate_system/coordinate_transformations/beam_axis_to_analyser_axis",
         "AXISNAME[analyser_take_off_polar]/@transformation_type": "rotation",
         "AXISNAME[analyser_take_off_polar]/@units":"@xps_token:instrument/analyser/analyser_take_off_polar/@units",
         "AXISNAME[analyser_take_off_polar]/@vector": [0, 0, 1],

         "AXISNAME[angle_beam_analyser]":"@xps_token:beam_xray/source_analyser_angle",
         "AXISNAME[angle_beam_analyser]/@depends_on":".",
         "AXISNAME[angle_beam_analyser]/@transformation_type": "rotation",
         "AXISNAME[angle_beam_analyser]/@units":"@xps_token:beam_xray/source_analyser_angle/@units",
         "AXISNAME[angle_beam_analyser]/@vector": [0, 0, 1]
       }
     }
   }, 
   "/ENTRY[entry]/SAMPLE[sample]":{
     "depends_on":"/entry/sample/transformations/sample_rotation",
     "TRANSFORMATIONS[transformations]": {
       "AXISNAME[sample_rotation]":"@xps_token:sample/sample_rotation",
       "AXISNAME[sample_rotation]/@depends_on":"sample_tilt_azimuth",
       "AXISNAME[sample_rotation]/@transformation_type": "rotation",
       "AXISNAME[sample_rotation]/@units":"@xps_token:sample/sample_rotation/@units",
       "AXISNAME[sample_rotation]/@vector": [1, 0, 0],

       "AXISNAME[sample_tilt_azimuth]":"@xps_token:sample/sample_tilt_azimuth",
       "AXISNAME[sample_tilt_azimuth]/@depends_on":"sample_tilt_polar",
       "AXISNAME[sample_tilt_azimuth]/@transformation_type": "rotation",
       "AXISNAME[sample_tilt_azimuth]/@units":"@xps_token:sample/sample_tilt_azimuth/@units",
       "AXISNAME[sample_tilt_azimuth]/@vector": [0, 1, 0],

       "AXISNAME[sample_tilt_polar]":"@xps_token:sample/sample_tilt_normal_polar",
       "AXISNAME[sample_tilt_polar]/@depends_on":"angle_beam_sample",
       "AXISNAME[sample_tilt_polar]/@transformation_type": "rotation",
       "AXISNAME[sample_tilt_polar]/@units":"@xps_token:sample/sample_tilt_normal_polar/@units",
       "AXISNAME[sample_tilt_polar]/@vector": [0, 0, 1],

       "AXISNAME[angle_beam_sample]":"@link:/entry/geometries/xps_coordinate_system/coordinate_transformations/beam_axis_to_analyser_axis"
    }
   }
 }