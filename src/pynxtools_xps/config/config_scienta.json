      },
      "POSITIONER[positioner_y]":{
        "value":"@eln",
        "value/@units":"@eln"
      },
      "POSITIONER[positioner_z]":{
        "value":"@eln",
        "value/@units":"@eln"
      },
      "POSITIONER[positioner_phi]":{
        "value":"@eln",
        "value/@units":"@eln"
      }
    },
    "pressure_gauge":{
      "name":"@eln",
      "measurement":"@eln",
      "type":"@eln",
      "value":"@eln",
      "value/@units":"@eln"
    },
    "flood_gun":{
      "name":"@eln",
      "actuation_target":"@eln",
      "type":"@eln",
      "current":"@eln",
      "current/@units":"@eln"
    }
