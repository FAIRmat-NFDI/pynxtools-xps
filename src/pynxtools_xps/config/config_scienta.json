        "type":"@eln",
        "value":"@eln",
        "value/@units":"@eln"
      },
      "sample_bias_voltmeter":{
        "name":"@eln",
        "measurement":"@eln",
        "type":"@eln",
        "value":"@eln",
        "value/@units":"@eln"
      },
      "sample_bias_potentiostat":{
        "name":"@eln",
        "actuation_target":"@eln",
        "type":"@eln",
