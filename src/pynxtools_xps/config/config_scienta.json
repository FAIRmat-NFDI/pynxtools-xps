  "/ENTRY/energy_axis_calibration":{
    "calibrated_axis":"@data:energy",
    "calibrated_axis/@units":"eV"
  },
  "/ENTRY/energy_referencing":null,
  "/ENTRY/transmission_correction":null
  },
  "/ENTRY/SAMPLE[sample]":{
    "name":"@attrs:sample/sample_name",
    "identifier":"@eln",
    "identifier/@type":"@eln",
    "atom_types":"@eln",
    "chemical_formula":"@eln",
    "physical_form":"@eln",
    "situation":"@eln",
    "history":{
      "sample_preparation":{
        "start_time":"@eln",
        "end_time":"@eln",
        "description":"@eln",
        "method":"@eln"
      }
    },
    "temperature_env":{
      "temperature_sensor":"@link:/entry/instrument/manipulator/temperature_sensor",
      "sample_heater":"@link:/entry/instrument/manipulator/sample_heater",
      "cryostat":null
    },
    "gas_pressure_env":{
      "pressure_gauge":"@link:/entry/instrument/pressure_gauge"
    },
    "bias_env":{
      "potentiostat":"@link:/entry/instrument/manipulator/sample_bias_potentiostat",
      "voltmeter":"@link:/entry/instrument/manipulator/sample_bias_voltmeter"
    },
    "drain_current_env":{
      "amperemeter":"@link:/entry/instrument/manipulator/drain_current_amperemeter"
    },
    "flood_gun_current_env":{
      "flood_gun":"@link:/entry/instrument/flood_gun"
    }
  },
  "/ENTRY/data":{
    "@axes":["energy"],
    "@signal":"data",
    "data":"@data:average",
    "FIELDNAME_errors[data_errors]":"@data:errors",
    "data/@units":"@attrs:region/intensity/@units",
    "DATA[*]":"@data:*.scans",
    "DATA[*]/@units":"@attrs:region/intensity/@units",
    "energy":"@data:energy",
    "energy/@type":"@attrs:region/energy_scale",
    "energy/@units":"@attrs:region/energy_axis/@units",
    "@energy_indices":0
  }
