          "name":"@eln",
          "physical_quantity":"@eln",
          "type":"@eln",
          "PID[pid]/setpoint":"@eln",
          "PID[pid]/setpoint/@units":"@eln"
        }
      },
      "pressure_gauge":{
        "name":"@eln",
        "measurement":"@eln",
        "type":"@eln",
        "value":"@eln",
        "value/@units":"@eln"
