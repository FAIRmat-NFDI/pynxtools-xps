    "/ENTRY/SAMPLE[sample]":{
      "name":"@attrs:parameters/Group",
      "identifier":{
        "identifier":"@eln"
      },
      "atom_types":"@eln",
      "physical_form":"@eln",
      "situation":"@eln",
