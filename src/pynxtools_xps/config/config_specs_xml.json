    "/ENTRY/data":{
      "@axes":["energy"],
      "@signal":"data",
      "data":"@data:average",
      "data_errors":"@data:errors",
      "data/@units":"counts",
      "DATA[*]":"@data:*.scans",
      "DATA[*]/@units":"counts",
      "energy":"@data:energy",
      "energy/@type":"kinetic",
      "energy/@units":"eV",
      "energy/@reference":null,
      "@energy_indices":0      
    }
